#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'anchorecli==0.4.0.dev0','console_scripts','anchore-cli'
__requires__ = 'anchorecli==0.4.0.dev0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('anchorecli==0.4.0.dev0', 'console_scripts', 'anchore-cli')()
    )
